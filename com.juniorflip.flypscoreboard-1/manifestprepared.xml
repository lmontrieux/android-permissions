<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.juniorflip.flypscoreboard">
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:label="@string/app_name" android:name=".SplashLogo">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:name=".Flypscoreboard">
            <intent-filter>
                <action android:name="com.juniorflip.flypscoreboard.Flypscoreboard"><nothingHere/></action>
                <category android:name="android.intent.category.DEFAULT"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:name=".IntentLauncher">
            <intent-filter>
                <action android:name="com.juniorflip.flypscoreboard.IntentLauncher"><nothingHere/></action>
                <category android:name="android.intent.category.DEFAULT"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:name=".GameDetails">
            <intent-filter>
                <action android:name="com.juniorflip.flypscoreboard.Gamedetails"><nothingHere/></action>
                <category android:name="android.intent.category.DEFAULT"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:name=".FullSchedule">
            <intent-filter>
                <action android:name="com.juniorflip.flypscoreboard.FullSchedule"><nothingHere/></action>
                <category android:name="android.intent.category.DEFAULT"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:name=".NewsList">
            <intent-filter>
                <action android:name="com.juniorflip.flypscoreboard.NewsList"><nothingHere/></action>
                <category android:name="android.intent.category.DEFAULT"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:name=".NewsFeedDetail">
            <intent-filter>
                <action android:name="com.juniorflip.flypscoreboard.NewsFeedDetail"><nothingHere/></action>
                <category android:name="android.intent.category.DEFAULT"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:name=".MasterWebView">
            <intent-filter>
                <action android:name="com.juniorflip.flypscoreboard.MasterWebView"><nothingHere/></action>
                <category android:name="android.intent.category.DEFAULT"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:label="@string/app_name" android:name=".FacebookHack">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
            </intent-filter>
        </activity>
        <activity android:label="@string/app_name" android:launchMode="singleInstance" android:name=".ShareGenerator">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW"><nothingHere/></action>
                <android-data android:host="com.juniorflip.FlypScoreboard" android:scheme="myapp"><nothingHere/></android-data>
            <category android:name="android.intent.category.DEFAULT"><nothingHere/></category>
                <category android:name="android.intent.category.BROWSABLE"><nothingHere/></category>
                </intent-filter>
        </activity>
        <activity android:configChanges="keyboard|keyboardHidden|orientation" android:name="com.google.ads.AdActivity"><nothingHere/></activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"><nothingHere/></uses-permission>
</manifest>
