<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="org.fuscu.android">
    <application android:icon="@drawable/icon" android:label="@string/app_name" android:name=".MobileBankingApp">
        <activity android:configChanges="keyboardHidden|orientation|screenSize" android:label="@string/title_activity_main" android:name=".MainActivity" android:theme="@style/CustomActivityTheme" android:windowSoftInputMode="adjustResize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:configChanges="keyboardHidden|orientation|screenSize" android:label="@string/title_activity_main" android:name="BankingView" android:theme="@android:style/Theme.NoTitleBar"><nothingHere/></activity>
        <activity android:configChanges="keyboardHidden|orientation|screenSize" android:label="@string/title_activity_main" android:name="VertifiActivity" android:theme="@style/CustomActivityTheme"><nothingHere/></activity>
        <activity android:configChanges="keyboardHidden|orientation" android:label="@string/app_name" android:name="com.vertifi.CameraActivity" android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.VIEW"><nothingHere/></action>
                <category android:name="android.intent.category.DEFAULT"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:name="org.fuscu.android.SendLogActivity" android:textAppearance="@android:style/TextAppearance.Large" android:theme="@android:style/Theme.Dialog" android:windowSoftInputMode="stateHidden">
            <intent-filter>
                <action android:name="com.pmsyscorp.SEND_LOG"><nothingHere/></action>
                <category android:name="android.intent.category.DEFAULT"><nothingHere/></category>
            </intent-filter>
        </activity>
    </application>
<uses-permission android:name="android.permission.INTERNET"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.WAKE_LOCK"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.CAMERA"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"><nothingHere/></uses-permission>
    </manifest>
