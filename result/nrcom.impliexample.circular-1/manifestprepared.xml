<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="nrcom.impliexample.circular">
    <application android:icon="@drawable/circularappicon" android:label="@string/app_name" android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
        <activity android:label="@string/title_activity_start_screen" android:name=".StartScreen" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:label="@string/title_activity_main" android:name=".Circular" android:screenOrientation="portrait">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:configChanges="keyboard|keyboardHidden|orientation" android:name="com.google.ads.AdActivity"><nothingHere/></activity>
    </application>
<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.INTERNET"><nothingHere/></uses-permission>
    </manifest>
