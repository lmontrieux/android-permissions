<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.Health.tutorials">
    <application android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
        <activity android:label="@string/app_name" android:name="com.Simple.tutorial.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|screenSize|smallestScreenSize|uiMode" android:name="com.google.android.gms.ads.AdActivity"><nothingHere/></activity>
        <activity android:configChanges="orientation" android:label="@string/app_name" android:name="com.Simple.tutorial.Webview" android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
            <intent-filter>
                <action android:name="android.intent.action.VIEW"><nothingHere/></action>
                <category android:name="android.intent.category.DEFAULT"><nothingHere/></category>
            </intent-filter>
        </activity>
    <meta-data android:name="com.google.android.gms.version" android:value="@integer/google_play_services_version"><nothingHere/></meta-data>
        </application>
    <uses-permission android:name="android.permission.INTERNET"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"><nothingHere/></uses-permission>
</manifest>
