<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.iHues.Armenian.radios">
    <application android:icon="@drawable/icon_app" android:label="@string/app_name">
        <activity android:label="@string/app_name" android:name="MainActivity" android:screenOrientation="portrait" android:theme="@android:style/Theme.NoTitleBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW"><nothingHere/></action>
                <android-data android:host="armenianradios.ihues.com" android:scheme="http"><nothingHere/></android-data>
                <category android:name="android.intent.category.DEFAULT"><nothingHere/></category>
                <category android:name="android.intent.category.BROWSABLE"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:label="@string/app_name" android:name="com.iHues.Radio.Player.RadioPlayer" android:screenOrientation="portrait" android:theme="@android:style/Theme.NoTitleBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
            </intent-filter>
        </activity>
        <activity android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|screenSize|smallestScreenSize|uiMode" android:name="com.google.ads.AdActivity"><nothingHere/></activity>
    </application>
<uses-permission android:name="android.permission.INTERNET"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"><nothingHere/></uses-permission>
    </manifest>
