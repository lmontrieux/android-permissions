<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="pl.sw.CrabIt">
    <application android:icon="@drawable/singelcrab" android:label="@string/app_name">
        <activity android:configChanges="keyboardHidden|orientation" android:name="pl.sw.Startup.CrabItStartScreen" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:label="@string/app_name" android:name=".CrabItGameMain" android:screenOrientation="portrait"><nothingHere/></activity>
        <activity android:label="@string/app_name" android:name=".CrabItLocalScore" android:screenOrientation="portrait">
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|screenSize|smallestScreenSize|uiMode" android:name="com.google.ads.AdActivity"><nothingHere/></activity>
    </application>
    <uses-permission android:name="android.permission.WAKE_LOCK"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.INTERNET"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"><nothingHere/></uses-permission>
</manifest>
