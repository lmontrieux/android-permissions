<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.sorapps.blackblue2">
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:label="@string/app_name" android:name=".info">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.INFO"><nothingHere/></category>
            </intent-filter>
        </activity>
        <receiver android:label="@string/app_name" android:name=".Widget">
            <intent-filter>
                <action android:name="android.appwidget.action.APPWIDGET_UPDATE"><nothingHere/></action>
            </intent-filter>
            <meta-data android:name="android.appwidget.provider" android:resource="@xml/widget"><nothingHere/></meta-data>
        </receiver>
    </application>
</manifest>
