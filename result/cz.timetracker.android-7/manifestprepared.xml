<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="cz.timetracker.android">
    <application android:debuggable="false" android:icon="@drawable/icon" android:label="@string/app_name" android:name=".Application" android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
        <activity android:name=".activity.WelcomeActivity" android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:name=".activity.CardLoginActivity" android:screenOrientation="landscape"><nothingHere/></activity>
        <activity android:name=".activity.NameLoginActivity" android:screenOrientation="landscape"><nothingHere/></activity>
        <activity android:name=".activity.WorkRoomActivity" android:screenOrientation="landscape"><nothingHere/></activity>
        <activity android:name=".activity.MainActivity" android:screenOrientation="landscape"><nothingHere/></activity>
        <activity android:name=".activity.SettingsActivity" android:screenOrientation="landscape"><nothingHere/></activity>
        <activity android:name=".activity.CompanySetupActivity" android:screenOrientation="landscape"><nothingHere/></activity>
        <receiver android:name=".messaging.BootUpReceiver">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED"><nothingHere/></action>
                <category android:name="android.intent.category.HOME"><nothingHere/></category>
            </intent-filter>
        </receiver>
        <receiver android:name=".messaging.BatteryReceiver">
            <intent-filter>
                <action android:name="android.intent.action.BATTERY_CHANGED"><nothingHere/></action>
            </intent-filter>
        </receiver>
    </application>
<uses-permission android:name="android.permission.INTERNET"><nothingHere/></uses-permission>
