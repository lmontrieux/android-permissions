<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.aayat.kol.simpletorch">
    <application android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:theme="@android:style/Theme.Translucent.NoTitleBar">
        <activity android:label="@string/app_name" android:name=".Torch" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:finishOnTaskLaunch="true" android:label="@string/app_name" android:name=".QuickTorch">
            <intent-filter>
                <action android:name="android.intent.action.SEARCH_LONG_PRESS"><nothingHere/></action>
                <category android:name="android.intent.category.DEFAULT"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|screenSize|smallestScreenSize|uiMode" android:name="com.google.android.gms.ads.AdActivity"><nothingHere/></activity>
        <meta-data android:name="com.google.android.gms.version" android:value="@integer/google_play_services_version"><nothingHere/></meta-data>
    </application>
    <uses-feature android:name="android.hardware.camera" android:required="false"><nothingHere/></uses-feature>
    <uses-feature android:name="android.hardware.touchscreen" android:required="false"><nothingHere/></uses-feature>
    <uses-permission android:name="android.permission.CAMERA"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.WAKE_LOCK"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.INTERNET"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"><nothingHere/></uses-permission>
</manifest>
