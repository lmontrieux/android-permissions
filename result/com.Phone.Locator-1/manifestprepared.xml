<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.Phone.Locator">
    <application android:icon="@drawable/icon" android:label="Phone Locator">
        <activity android:label="Phone Locator" android:name=".Phone_Locator">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:label="Phone Locator" android:name=".dept"><nothingHere/></activity>
        <activity android:label="Phone Locator" android:name=".DeptList" android:theme="@style/FullHeightDialog"><nothingHere/></activity>
        <activity android:label="Phone Locator" android:name=".Keyword"><nothingHere/></activity>
        <activity android:label="Phone Locator" android:name=".KeywordList" android:theme="@style/FullHeightDialog"><nothingHere/></activity>
    </application>
<uses-permission android:name="android.permission.CALL_PHONE"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.INTERNET"><nothingHere/></uses-permission>
    </manifest>
