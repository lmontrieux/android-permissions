<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.appscanner.scanner">
    <application android:icon="@drawable/launcher_icon" android:label="@string/app_name">
        <activity android:name=".MainWindowActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:name=".OldScansActivity"><nothingHere/></activity>
        <activity android:label="@string/app_name" android:name=".ScannerActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
    </application>
</manifest>
