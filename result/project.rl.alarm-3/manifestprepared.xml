<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="project.rl.alarm">
    <application android:allowBackup="true" android:icon="@drawable/icon" android:label="@string/app_name" android:theme="@android:style/Theme.DeviceDefault.Light.NoActionBar">
        <activity android:label="@string/app_name" android:name="project.rl.alarm.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:name="project.rl.alarm.AlarmActivity">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:name="project.rl.alarm.SetAlarmActivity" android:windowSoftInputMode="adjustPan"><nothingHere/></activity>
        <activity android:name="project.rl.alarm.SetDeleteActivity"><nothingHere/></activity>
        <activity android:name="project.rl.alarm.SetTimeActivity"><nothingHere/></activity>
        <activity android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|screenSize|smallestScreenSize|uiMode" android:name="com.google.android.gms.ads.AdActivity"><nothingHere/></activity>
    <meta-data android:name="com.google.android.gms.version" android:value="@integer/google_play_services_version"><nothingHere/></meta-data>
        <receiver android:name="project.rl.alarm.AlarmReceiver"><nothingHere/></receiver>
        <receiver android:name="project.rl.alarm.AlarmBootReceiver">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED"><nothingHere/></action>
            </intent-filter>
        </receiver>
        </application>
<uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.VIBRATE"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.WAKE_LOCK"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.WRITE_SETTINGS"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.DISABLE_KEYGUARD"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.INTERNET"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"><nothingHere/></uses-permission>
    </manifest>
