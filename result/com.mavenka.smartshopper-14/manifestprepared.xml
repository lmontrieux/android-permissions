<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.mavenka.smartshopper">
    <application android:allowBackup="true" android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:theme="@style/AppTheme">
        <activity android:label="@string/app_name" android:name="com.mavenka.smartshopper.ItemListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:label="@string/action_settings" android:name="com.mavenka.smartshopper.SettingsActivity" android:parentActivityName="com.mavenka.smartshopper.ItemListActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value="com.mavenka.smartshopper.ItemListActivity"><nothingHere/></meta-data>
        </activity>
    </application>
</manifest>
