<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="tw.com.dayhotel">
    <application android:icon="@drawable/icon" android:label="@string/app_name" android:theme="@style/myTheme">
        <activity android:label="@string/app_name" android:name=".index" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:label="@string/app_name" android:name=".main" android:screenOrientation="portrait"><nothingHere/></activity>
        <activity android:configChanges="keyboardHidden|orientation" android:name=".map_view" android:screenOrientation="portrait"><nothingHere/></activity>
        <activity android:configChanges="keyboardHidden|orientation" android:name=".near_list" android:screenOrientation="portrait"><nothingHere/></activity>
        <uses-library android:name="com.google.android.maps"><nothingHere/></uses-library>
    </application>
    <uses-permission android:name="android.permission.INTERNET"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.CALL_PRIVILEGED"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"><nothingHere/></uses-permission>
</manifest>
