<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.blogspot.flawedbot.checkreferrer">
    <application android:icon="@drawable/ic_launcher" android:label="@string/app_name">
        <activity android:label="@string/app_name" android:name="CheckReferrer">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
    <receiver android:exported="true" android:name="com.crittermap.backcountrynavigator.ReferringReceiver">
            <intent-filter>
                <action android:name="com.android.vending.INSTALL_REFERRER"><nothingHere/></action>
            </intent-filter>
        </receiver>
        </application>
</manifest>
