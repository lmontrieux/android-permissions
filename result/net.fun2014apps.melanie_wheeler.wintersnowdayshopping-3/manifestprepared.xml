<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="net.fun2014apps.melanie_wheeler.wintersnowdayshopping">
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:label="@string/app_name" android:name="net.fun2014apps.melanie_wheeler.wintersnowdayshopping.MainActivity" android:screenOrientation="landscape" android:theme="@android:style/Theme.Light.NoTitleBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|screenSize|smallestScreenSize|uiMode" android:name="com.google.ads.AdActivity"><nothingHere/></activity>
        <receiver android:name=".wintersnowdayshopping.BootReceiver">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED"><nothingHere/></action>
                <category android:name="android.intent.category.HOME"><nothingHere/></category>
            </intent-filter>
        </receiver>
    </application>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.INTERNET"><nothingHere/></uses-permission>
</manifest>
