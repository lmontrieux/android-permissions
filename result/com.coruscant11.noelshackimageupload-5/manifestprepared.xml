<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.coruscant11.noelshackimageupload">
    <application android:allowBackup="true" android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:theme="@style/Theme.Noelshack">
        <activity android:label="@string/app_name" android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:label="@string/title_activity_upload_picture" android:name="com.coruscant11.noelshackimageupload.UploadPicture">
            <intent-filter>
                <action android:name="android.intent.action.SEND"><nothingHere/></action>
                <android-data android:mimeType="image/*"><nothingHere/></android-data>
            <category android:name="android.intent.category.DEFAULT"><nothingHere/></category>
                </intent-filter>
        </activity>
    </application>
<uses-permission android:name="android.permission.ACCESS_WIFI_STATE"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.INTERNET"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.CAMERA"><nothingHere/></uses-permission>
    </manifest>
