<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.gl.android.filter">
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:label="@string/app_name" android:name=".MarketFilterActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
    </application>
    <supports-screens android:smallScreens="false"><nothingHere/></supports-screens>
</manifest>
