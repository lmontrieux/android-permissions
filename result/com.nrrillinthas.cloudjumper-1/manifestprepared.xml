<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.nrrillinthas.cloudjumper">
    <application android:allowBackup="true" android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:theme="@style/AppTheme">
        <activity android:label="@string/app_name" android:name="com.nrrillinthas.cloudjumper.MainMenu" android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:label="@string/title_activity_main" android:name="com.nrrillinthas.cloudjumper.MainActivity" android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.RUN"><nothingHere/></action>
                <android-data android:path="com.nrrillinthas.cloudjumper.MainActivity"><nothingHere/></android-data>
            <category android:name="android.intent.category.DEFAULT"><nothingHere/></category>
                </intent-filter>
        </activity>
        <activity android:label="@string/title_activity_game_over" android:name="com.nrrillinthas.cloudjumper.GameOver" android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.GAMEOVER"><nothingHere/></action>
                <android-data android:path="com.nrrillinthas.cloudjumper.GameOver"><nothingHere/></android-data>
            <category android:name="android.intent.category.DEFAULT"><nothingHere/></category>
                </intent-filter>
        </activity>
        <activity android:label="@string/title_activity_pause" android:name="com.nrrillinthas.cloudjumper.Pause">
            <intent-filter>
                <action android:name="android.intent.action.PAUSE"><nothingHere/></action>
                <category android:name="android.intent.category.DEFAULT"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:label="@string/title_activity_directions" android:name="com.nrrillinthas.cloudjumper.Directions">
            <intent-filter>
                <action android:name="android.intent.action.DIRECTIONS"><nothingHere/></action>
                <category android:name="android.intent.category.DEFAULT"><nothingHere/></category>
            </intent-filter>
        </activity>
    </application>
<uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.WAKE_LOCK"><nothingHere/></uses-permission>
    </manifest>
