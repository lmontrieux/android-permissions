<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.nairabir.bpm">
    <application android:allowBackup="true" android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:theme="@style/AppTheme">
        <activity android:label="@string/app_name" android:name="com.nairabir.bpm.MainWindow">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:label="@string/app_name" android:name="com.nairabir.bpm.ListActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW"><nothingHere/></action>
                <android-data android:host="tw90bpm" android:scheme="oauth"><nothingHere/></android-data>
            <category android:name="android.intent.category.DEFAULT"><nothingHere/></category>
                <category android:name="android.intent.category.BROWSABLE"><nothingHere/></category>
                </intent-filter>
        </activity>
        <activity android:hardwareAccelerated="true" android:label="@string/app_name" android:name="com.nairabir.bpm.ItemDetail"><nothingHere/></activity>
        <activity android:name="com.facebook.LoginActivity"><nothingHere/></activity>
        <activity android:label="Image gallery" android:name="com.nairabir.bpm.ItemGallery"><nothingHere/></activity>
        <meta-data android:name="com.facebook.sdk.ApplicationId" android:value="@string/app_id"><nothingHere/></meta-data>
    </application>
<uses-permission android:name="android.permission.INTERNET"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"><nothingHere/></uses-permission>
    </manifest>
