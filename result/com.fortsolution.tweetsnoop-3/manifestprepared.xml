<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.fortsolution.tweetsnoop">
    <application android:icon="@drawable/icon" android:label="@string/app_name" android:theme="@android:style/Theme.NoTitleBar">
        <activity android:label="@string/title_activity_dash_board" android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:launchMode="singleTask" android:name=".tweeter.PrepareRequestTokenActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.VIEW"><nothingHere/></action>
                <android-data android:host="callback" android:scheme="twitter-OAUTH-tweetsnoop-app"><nothingHere/></android-data>
            <category android:name="android.intent.category.DEFAULT"><nothingHere/></category>
                <category android:name="android.intent.category.BROWSABLE"><nothingHere/></category>
                </intent-filter>
        </activity>
        <activity android:name=".DashBoard" android:screenOrientation="portrait"><nothingHere/></activity>
        <activity android:name=".Instruction" android:screenOrientation="portrait"><nothingHere/></activity>
        <activity android:name=".Share" android:screenOrientation="portrait"><nothingHere/></activity>
        <activity android:name=".ShowTweet" android:screenOrientation="portrait"><nothingHere/></activity>
        <activity android:name=".TweetDetail" android:screenOrientation="portrait"><nothingHere/></activity>
        <activity android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|screenSize|smallestScreenSize|uiMode" android:name="com.google.ads.AdActivity"><nothingHere/></activity>
    <uses-library android:name="com.google.android.maps"><nothingHere/></uses-library>
        </application>
    <uses-permission android:name="android.permission.INTERNET"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.VIBRATE"><nothingHere/></uses-permission>
</manifest>
