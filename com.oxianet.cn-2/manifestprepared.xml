<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.oxianet.cn">
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:configChanges="keyboardHidden|orientation" android:label="@string/app_name" android:name=".MainActivity" android:screenOrientation="sensor" android:theme="@android:style/Theme.NoTitleBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:name="com.oxianet.cn.RegistrationAccountActivity" android:theme="@android:style/Theme.NoTitleBar"><nothingHere/></activity>
        <receiver android:name="com.oxianet.cn.NotificationsReceiver" android:permission="com.google.android.c2dm.permission.SEND">
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE"><nothingHere/></action>
                <category android:name="com.oxianet.cn"><nothingHere/></category>
            </intent-filter>
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.REGISTRATION"><nothingHere/></action>
                <category android:name="com.oxianet.cn"><nothingHere/></category>
            </intent-filter>
        </receiver>
    </application>
    <permission android:name="com.oxianet.cn.permission.C2D_MESSAGE" android:protectionLevel="signature"><nothingHere/></permission>
<uses-permission android:name="android.permission.INTERNET"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.GET_ACCOUNTS"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"><nothingHere/></uses-permission>
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE"><nothingHere/></uses-permission>
    <uses-permission android:name="com.oxianet.cn.permission.C2D_MESSAGE"><nothingHere/></uses-permission>
    </manifest>
