        <activity android:name="com.shimaotech.spamtrap.activities.SpamNumbersActivity"><nothingHere/></activity>
        <activity android:name="com.shimaotech.spamtrap.activities.SpamWordsActivity"><nothingHere/></activity>
        <activity android:name="com.shimaotech.spamtrap.activities.HelpActivity"><nothingHere/></activity>
