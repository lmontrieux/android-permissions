        <activity android:name="com.shimaotech.spamtrap.activities.SuspiciousTextViewActivity"><nothingHere/></activity>
        <receiver android:name="com.shimaotech.spamtrap.TextSentinel">
            <intent-filter android:priority="100">
                <action android:name="android.provider.Telephony.SMS_RECEIVED"><nothingHere/></action>
            </intent-filter>
        </receiver>
        <service android:enabled="true" android:name="com.shimaotech.spamtrap.services.ReporterService"><nothingHere/></service>
        <service android:enabled="true" android:name="com.shimaotech.spamtrap.services.SuggestionService"><nothingHere/></service>
