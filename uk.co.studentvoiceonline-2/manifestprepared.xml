<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="uk.co.studentvoiceonline">
    <application android:icon="@drawable/svicon" android:label="@string/app_name" android:theme="@android:style/Theme.Light">
        <activity android:label="@string/app_name" android:name=".android.activity.Welcome">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:label="London Universities" android:name=".android.activity.ViewUniversities">
            <intent-filter>
                <action android:name="co.uk.studentvoice.intent.VIEW_UNIVERSITIES"><nothingHere/></action>
                <category android:name="co.uk.studentvoice.intent.VIEW_UNIVERSITIES"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:label="About This University" android:name=".android.activity.ViewUniversityDetails"><nothingHere/></activity>
        <activity android:label="Local Attractions" android:name=".android.activity.FindLocalAttractions"><nothingHere/></activity>
    </application>
</manifest>
