<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="iq.gov.basra">
    <application android:allowBackup="true" android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:name="ActivityClass">
        <activity android:name=".Splash" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAINACTIVITY"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:name=".Second" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.SECOND"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
        <receiver android:name="com.parse.ParseBroadcastReceiver">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED"><nothingHere/></action>
                <action android:name="android.intent.action.USER_PRESENT"><nothingHere/></action>
            </intent-filter>
        </receiver>
        <receiver android:name="com.parse.GcmBroadcastReceiver" android:permission="com.google.android.c2dm.permission.SEND">
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE"><nothingHere/></action>
                <action android:name="com.google.android.c2dm.intent.REGISTRATION"><nothingHere/></action>
                <category android:name="iq.gov.basra"><nothingHere/></category>
            </intent-filter>
        </receiver>
    <service android:name="com.parse.PushService"><nothingHere/></service>
        </application>
<permission android:name="iq.gov.basra.permission.C2D_MESSAGE" android:protectionLevel="signature"><nothingHere/></permission>
    <uses-permission android:name="android.permission.INTERNET"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.WAKE_LOCK"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.VIBRATE"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.GET_ACCOUNTS"><nothingHere/></uses-permission>
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE"><nothingHere/></uses-permission>
    <uses-permission android:name="iq.gov.basra.permission.C2D_MESSAGE"><nothingHere/></uses-permission>
    </manifest>
