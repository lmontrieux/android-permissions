            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:exported="false" android:label="@string/app_name" android:name="com.liamw.root.addafeature.MainActivity">
            <intent-filter>
                <action android:name="com.liamw.root.addafeature.MAINACTIVITY"><nothingHere/></action>
            </intent-filter>
        </activity>
        <activity android:exported="false" android:label="@string/app_name" android:name="com.liamw.root.addafeature.DoneActivity">
            <intent-filter>
                <action android:name="com.liamw.root.addafeature.DONEACTIVITY"><nothingHere/></action>
            </intent-filter>
        </activity>
    </application>
</manifest>
