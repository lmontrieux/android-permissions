<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.damnation.BlockZombie">
    <application android:icon="@drawable/icon" android:label="@string/app_name" android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
        <activity android:clearTaskOnLaunch="true" android:name="Main" android:screenOrientation="portrait" launchMode="singleInstance" taskAffinity="FLAG_ACTIVITY_CLEAR_TOP">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:finishOnTaskLaunch="true" android:name="Game" android:screenOrientation="portrait" launchMode="singleInstance" taskAffinity="FLAG_ACTIVITY_CLEAR_TOP, FLAG_ACTIVITY_NO_HISTORY"><nothingHere/></activity>
        <activity android:finishOnTaskLaunch="true" android:name="Rank" android:screenOrientation="portrait" launchMode="singleInstance" taskAffinity="FLAG_ACTIVITY_CLEAR_TOP, FLAG_ACTIVITY_NO_HISTORY"><nothingHere/></activity>
        <activity android:finishOnTaskLaunch="true" android:name="Help" android:screenOrientation="portrait" launchMode="singleInstance" taskAffinity="FLAG_ACTIVITY_CLEAR_TOP, FLAG_ACTIVITY_NO_HISTORY"><nothingHere/></activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.VIBRATE"><nothingHere/></uses-permission>
</manifest>
