<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.servotronicstech.TVCNEWS">
    <application android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:theme="@android:style/Theme">
        <activity android:label="@string/app_name" android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:label="@string/live" android:name=".TVCNEWS" android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="com.servotronicstech.TVCNEWS.TVCNEWS"><nothingHere/></action>
                <category android:name="android.intent.category.DEFAULT"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:configChanges="keyboardHidden|orientation" android:label="@string/web" android:name=".TVCWebSite">
            <intent-filter>
                <action android:name="com.servotronicstech.TVCNEWS.TVCWEBSITE"><nothingHere/></action>
                <category android:name="android.intent.category.DEFAULT"><nothingHere/></category>
            </intent-filter>
        </activity>
    </application>
<uses-permission android:name="android.permission.INTERNET"><nothingHere/></uses-permission>
    </manifest>
