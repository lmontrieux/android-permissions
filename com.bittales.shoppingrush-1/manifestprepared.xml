<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:installLocation="auto" package="com.bittales.shoppingrush">
    <application android:debuggable="true" android:icon="@drawable/icon" android:label="Shopping Rush">
        <activity android:configChanges="keyboard|keyboardHidden|orientation" android:label="Shopping Rush" android:name="MainActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|screenSize|smallestScreenSize|uiMode" android:name="com.google.ads.AdActivity"><nothingHere/></activity>
        <receiver android:name="com.blundell.test.BillingReceiver">
            <intent-filter>
                <action android:name="com.android.vending.billing.IN_APP_NOTIFY"><nothingHere/></action>
                <action android:name="com.android.vending.billing.RESPONSE_CODE"><nothingHere/></action>
                <action android:name="com.android.vending.billing.PURCHASE_STATE_CHANGED"><nothingHere/></action>
            </intent-filter>
        </receiver>
    <service android:name="com.blundell.test.BillingService"><nothingHere/></service>
        </application>
    <uses-permission android:name="android.permission.WAKE_LOCK"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.INTERNET"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.VIBRATE"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"><nothingHere/></uses-permission>
</manifest>
