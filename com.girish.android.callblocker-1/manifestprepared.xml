<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.girish.android.callblocker">
    <application android:icon="@drawable/icon_block" android:label="@string/app_name" android:theme="@style/AppTheme">
        <activity android:icon="@drawable/icon_block" android:label="@string/app_name" android:name="com.girish.android.callblocker.Firstsplashscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:label="@string/app_name" android:name="com.girish.android.callblocker.NumberList" android:theme="@android:style/Theme.Light"><nothingHere/></activity>
        <activity android:label="@string/app_name" android:name="com.girish.android.callblocker.ListActivity"><nothingHere/></activity>
        <activity android:label="@string/app_name" android:name="com.girish.android.callblocker.Blockers"><nothingHere/></activity>
        <activity android:name="com.startapp.android.publish.list3d.List3DActivity" android:taskAffinity="com.example.android.androidtutorial.AppWall" android:theme="@android:style/Theme"><nothingHere/></activity>
        <activity android:configChanges="keyboardHidden|orientation|screenSize" android:name="com.startapp.android.publish.AppWallActivity" android:taskAffinity="com.example.android.androidtutorial.AppWall" android:theme="@android:style/Theme.Translucent"><nothingHere/></activity>
    <receiver android:name="com.girish.android.callblocker.Blocker">
            <intent-filter android:priority="1000">
                <action android:name="android.intent.action.PHONE_STATE"><nothingHere/></action>
            </intent-filter>
        </receiver>
        </application>
<uses-permission android:name="android.permission.CALL_PHONE"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.READ_CONTACTS"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.INTERNET"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"><nothingHere/></uses-permission>
    </manifest>
