<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:anyDensity="true" android:installLocation="preferExternal" android:resizeable="true" package="com.pumpkynhead.fuelwatch">
    <application android:icon="@drawable/fuel_marker_96" android:label="@string/app_name" android:theme="@style/Theme.Notitle">
        <activity android:configChanges="keyboardHidden|orientation" android:label="@string/app_name" android:name=".FuelWatchStart">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:configChanges="keyboardHidden|orientation" android:label="About" android:name=".extras.About">
            <intent-filter>
                <action android:name="android.intent.action.VIEW"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
        <uses-library android:name="com.google.android.maps" android:required="true"><nothingHere/></uses-library>
    </application>
    <uses-permission android:name="android.permission.INTERNET"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"><nothingHere/></uses-permission>
</manifest>
