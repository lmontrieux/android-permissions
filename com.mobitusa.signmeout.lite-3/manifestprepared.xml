<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.mobitusa.signmeout.lite">
    <application android:icon="@drawable/logo" android:label="@string/app_name" android:name="SignMeOutAppDelegate">
        <activity android:label="@string/app_name" android:name=".SplashScreenActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:name=".SignMeOut" android:screenOrientation="portrait"><nothingHere/></activity>
        <activity android:name=".AccountDetailsActivity" android:screenOrientation="portrait"><nothingHere/></activity>
        <activity android:name=".AccountSettingsActivity" android:screenOrientation="portrait"><nothingHere/></activity>
        <activity android:name=".AboutActivity" android:screenOrientation="portrait"><nothingHere/></activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"><nothingHere/></uses-permission>
</manifest>
