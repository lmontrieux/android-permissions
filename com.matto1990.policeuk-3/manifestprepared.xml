<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.matto1990.policeuk">
    <application android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:name=".PoliceApplication" android:theme="@android:style/Theme.Light.NoTitleBar">
        <activity android:label="@string/app_name" android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:name=".ForcesActivity"><nothingHere/></activity>
        <activity android:name=".ForceActivity"><nothingHere/></activity>
        <activity android:name=".NeighbourhoodActivity"><nothingHere/></activity>
        <activity android:name=".CrimeActivity"><nothingHere/></activity>
        <activity android:name=".EventActivity"><nothingHere/></activity>
        <activity android:name=".SearchActivity">
            <intent-filter>
                <action android:name="android.intent.action.SEARCH"><nothingHere/></action>
            </intent-filter>
            <meta-data android:name="android.app.searchable" android:resource="@xml/searchable"><nothingHere/></meta-data>
        </activity>
        <meta-data android:name="android.app.default_searchable" android:value=".SearchActivity"><nothingHere/></meta-data>
        <provider android:authorities="com.matto1990.policeuk.SearchSuggestionProvider" android:name=".SearchSuggestionProvider"><nothingHere/></provider>
    </application>
    <uses-permission android:name="android.permission.INTERNET"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"><nothingHere/></uses-permission>
</manifest>
