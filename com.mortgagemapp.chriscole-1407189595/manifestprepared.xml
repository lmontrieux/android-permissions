<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.mortgagemapp.chriscole">
    <application android:allowBackup="true" android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:theme="@style/AppTheme">
        <activity android:label="@string/app_name" android:name="com.mortgagemapp.appgenwebview.SplashActivity" android:theme="@android:style/Theme.NoTitleBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:name="com.mortgagemapp.appgenwebview.WebviewActivity" android:theme="@android:style/Theme.NoTitleBar"><nothingHere/></activity>
        <receiver android:exported="true" android:name="com.mortgagemapp.appgenwebview.ReferrerReceiver">
            <intent-filter>
                <action android:name="com.android.vending.INSTALL_REFERRER"><nothingHere/></action>
            </intent-filter>
        </receiver>
    </application>
<uses-permission android:name="android.permission.INTERNET"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.READ_PROFILE"><nothingHere/></uses-permission>
    </manifest>
