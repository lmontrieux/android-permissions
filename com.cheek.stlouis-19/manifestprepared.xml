<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.cheek.stlouis">
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:configChanges="keyboardHidden|orientation" android:name=".StLouis" android:screenOrientation="portrait" android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
        <receiver android:name="com.cheek.iap.BillingReceiver">
            <intent-filter>
                <action android:name="com.android.vending.billing.IN_APP_NOTIFY"><nothingHere/></action>
                <action android:name="com.android.vending.billing.RESPONSE_CODE"><nothingHere/></action>
                <action android:name="com.android.vending.billing.PURCHASE_STATE_CHANGED"><nothingHere/></action>
            </intent-filter>
        </receiver>
    <service android:name="com.cheek.iap.BillingService"><nothingHere/></service>
        </application>
    <uses-permission android:name="android.permission.INTERNET"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"><nothingHere/></uses-permission>
    <uses-permission android:name="com.android.vending.BILLING"><nothingHere/></uses-permission>
</manifest>
