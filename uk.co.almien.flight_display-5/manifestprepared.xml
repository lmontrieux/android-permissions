<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="uk.co.almien.flight_display">
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:label="@string/app_name" android:name=".FlightDisplayActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:label="Flight display options" android:name="options"><nothingHere/></activity>
    </application>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.WAKE_LOCK"><nothingHere/></uses-permission>
</manifest>
