<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.primcast.dwarkadhish.org">
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:configChanges="keyboardHidden|orientation" android:label="@string/app_name" android:name="SRVideoActivity" android:screenOrientation="portrait" android:theme="@android:style/Theme.NoTitleBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:label="@string/app_name" android:name=".SRVideoPlayer" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name=".SRVideoPlayer"><nothingHere/></action>
                <category android:name="android.intent.category.DEFAULT"><nothingHere/></category>
            </intent-filter>
        </activity>
    </application>
    <uses-feature android:name="android.hardware.touchscreen" android:required="true"><nothingHere/></uses-feature>
    <uses-feature android:name="android.hardware.telephony" android:required="false"><nothingHere/></uses-feature>
<uses-permission android:name="android.permission.INTERNET"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"><nothingHere/></uses-permission>
    </manifest>
