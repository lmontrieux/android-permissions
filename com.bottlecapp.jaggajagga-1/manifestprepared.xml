<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.bottlecapp.jaggajagga">
    <application android:allowBackup="true" android:icon="@drawable/icon" android:label="@string/app_name" android:theme="@style/AppTheme">
        <activity android:label="@string/app_name" android:name="com.bottlecapp.jaggajagga.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:label="@string/app_name" android:name=".MainActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="com.bottlecapp.jaggajagga.MainActivity"><nothingHere/></action>
                <category android:name="android.intent.category.DEFAULT"><nothingHere/></category>
            </intent-filter>
        </activity>
    </application>
<uses-permission android:name="android.permission.INTERNET"><nothingHere/></uses-permission>
    </manifest>
