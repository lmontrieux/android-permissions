<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.s.phatos.ezplayer">
    <application android:allowBackup="true" android:icon="@drawable/app_icon_small" android:label="@string/app_name" android:theme="@android:style/Theme.NoTitleBar">
        <activity android:launchMode="singleTask" android:name="com.s.phatos.ezplayer.view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
        <service android:name="com.s.phatos.ezplayer.service.PlayerService">
            <intent-filter>
                <action android:name="com.s.phatos.ezplayer.service.playerService"><nothingHere/></action>
                <category android:name="android.intent.category.DEFAULT"><nothingHere/></category>
            </intent-filter>
        </service>
    </application>
<uses-permission android:name="android.permission.READ_PHONE_STATE"><nothingHere/></uses-permission>
    </manifest>
