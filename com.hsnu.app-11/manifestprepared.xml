<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.hsnu.app">
    <application android:allowBackup="true" android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:theme="@style/AppTheme">
        <activity android:label="@string/app_name" android:name="com.hsnu.app.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
            <meta-data android:name="android.app.default_searchable" android:value=".SearchActivity"><nothingHere/></meta-data>
        </activity>
        <activity android:label="@string/title_activity_search" android:name=".SearchActivity">
            <intent-filter>
                <action android:name="android.intent.action.SEARCH"><nothingHere/></action>
            </intent-filter>
            <meta-data android:name="android.app.searchable" android:resource="@xml/searchable"><nothingHere/></meta-data>
        </activity>
        <activity android:label="@string/action_settings" android:name=".SettingsActivity"><nothingHere/></activity>
        <activity android:label="@string/title_activity_bulletin" android:name=".BulletinActivity"><nothingHere/></activity>
    </application>
<uses-permission android:name="android.permission.INTERNET"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"><nothingHere/></uses-permission>
    </manifest>
