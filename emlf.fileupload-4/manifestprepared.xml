<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="emlf.fileupload">
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:configChanges="keyboardHidden|orientation" android:label="@string/app_name" android:name=".mainactivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.SEND"><nothingHere/></action>
                <android-data android:mimeType="image/*"><nothingHere/></android-data>
            <category android:name="android.intent.category.DEFAULT"><nothingHere/></category>
                </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.SEND"><nothingHere/></action>
                <android-data android:mimeType="video/*"><nothingHere/></android-data>
            <category android:name="android.intent.category.DEFAULT"><nothingHere/></category>
                </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.SEND"><nothingHere/></action>
                <android-data android:mimeType="audio/*"><nothingHere/></android-data>
            <category android:name="android.intent.category.DEFAULT"><nothingHere/></category>
                </intent-filter>
        </activity>
        <activity android:configChanges="keyboardHidden|orientation" android:label="Upload Progress" android:name=".uploadactivity" class=".uploadactivity"><nothingHere/></activity>
    </application>
<uses-permission android:name="android.permission.INTERNET"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.READ_CONTACTS"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.GET_ACCOUNTS"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.WAKE_LOCK"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"><nothingHere/></uses-permission>
    </manifest>
