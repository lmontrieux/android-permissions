<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.deadkitty.carmasutra">
    <application android:icon="@drawable/icon" android:label="@string/app_name" android:theme="@style/CarmasutraTheme">
        <activity android:label="@string/app_name" android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
                <category android:name="android.intent.category.LAUNCHER"><nothingHere/></category>
            </intent-filter>
        </activity>
        <activity android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|screenSize|smallestScreenSize|uiMode" android:label="@string/app_name" android:launchMode="singleTask" android:name=".Main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"><nothingHere/></action>
            </intent-filter>
            <intent-filter><nothingHere/></intent-filter>
        </activity>
        <activity android:name=".PosturaActivity"><nothingHere/></activity>
        <activity android:launchMode="singleInstance" android:name=".TwitterLogin">
            <intent-filter>
                <action android:name="android.intent.action.VIEW"><nothingHere/></action>
                <android-data android:host="twitter" android:scheme="carmasutra"><nothingHere/></android-data>
            <category android:name="android.intent.category.DEFAULT"><nothingHere/></category>
                <category android:name="android.intent.category.BROWSABLE"><nothingHere/></category>
                </intent-filter>
        </activity>
        <activity android:name=".FacebookLoginActivity"><nothingHere/></activity>
        <activity android:name=".ListaPosturasActivity"><nothingHere/></activity>
        <activity android:name="com.facebook.LoginActivity"><nothingHere/></activity>
        <meta-data android:name="com.facebook.sdk.ApplicationId" android:value="@string/app_id"><nothingHere/></meta-data>
    </application>
<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.INTERNET"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"><nothingHere/></uses-permission>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"><nothingHere/></uses-permission>
    </manifest>
